// Bu Prisma sxemasi fayli.
// Sintaksis haqida ko'proq ma'lumot olish uchun: https://pris.ly/d/prisma-schema
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id             String   @id @default(uuid())
  productId      String   @unique
  audioTextUrl   String?
  audioUrl       String?
  captionUrl     String?
  videoCaptionUrl String?
  videoUrl       String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
